<?xml version="1.0" encoding="UTF-8"?>

<data>
  <area_data>

      <area name="area1">
      <N>100000</N>         <!-- Population -->
      <rho_V>0.5</rho_V>    <!-- Initial vaccination rate -->
      <rho_I_N>10</rho_I_N> <!-- Initial cases per day = rho_I * N -->
      <delta_r>0</delta_r>  <!-- Testing effect on rate out of I -->
      <gamma>1.4</gamma>    <!-- Behavior infection multiplier -->
      <rho>1</rho>          <!-- Proportion willing to be vaccinated -->
      </area>

      <area name="area2">
      <N>100000</N> 
      <rho_V>0.1</rho_V> 
      <rho_I_N>25</rho_I_N> 
      <delta_r>0</delta_r> 
      <gamma>0.7</gamma> 
      <rho>1</rho> 
      </area>

    <donor>area1</donor>   <!-- Donor area -->
    <m>area2</m>           <!-- Variant area -->
    <n>500</n>           <!-- person infection-days until new variant -->
  </area_data>

  <scenario_data>
    <r_I>0.2</r_I>         <!-- rate out of exposed (E) state into infectious (I) state -->
    <r_0>0.286571</r_0>    <!-- rate out of infectious state without testing -->
    <r_R>0.0666667</r_R>   <!-- rate out of state hospitalized (H) into recovery or death -->
<!--<p_V_H>0.15</p_V_H>         P(Hospitalized | Infected and Vaxxed)
    <p_H>0.3</p_H>              P(Hospitalized | Infected) 
    <p_D>0.1</p_D>              P(Dead | Hospitalized) -->
    <p_D>0.03</p_D>        <!--  P(Dead | Infected) -->
    <p_V_D>0.003</p_V_D>   <!--  P(Dead | Infected and Vaxxed ) -->    
    <a_0>0.6</a_0>         <!-- Initial infection rate (proportion/day) -->
    <delta_a>0.6</delta_a> <!-- Change in infection rate for a new variant (proportion/day) -->
    <p_e>0.6</p_e>         <!-- 0.6 Transmission rate from a vacc person (prop of unvacc rate) -->
    <p_r>0.6</p_r>         <!-- 0.6 Infection rate for a vacc person (proportion of unvacc rate) -->
    <L>5</L>              <!-- 20 Lag for variant to reach other areas, days -->
    <T_D>1</T_D>          <!-- 45 Time for variant to dominate -->
    <p>0.01</p>            <!-- prop of infected that have new variant when introduced -->
    <T>20</T>             <!-- Time horizon, days -->
    <B_0>10000</B_0>         <!-- Vaccine available day 1 -->
    <b></b>                <!-- Vaccine avail as a prop, day = 0,...,T-1. Default = 1 -->
    <!-- 
      to add b here, just seperate the numbers by commas
      ex. <b>1,1,1,1,1</b>
    -->
  </scenario_data>

  <params>
    <verbosity>3</verbosity>              <!-- 0 least verbose output ... 3 most verbose -->
    <simulate_only>0</simulate_only>      <!-- If 1, only do simulate-->
    <improving>0</improving>              <!-- 1 improving search, inner loop-->
    <realloc_flag>1</realloc_flag>        <!-- reallocation of unused vacc to non-donors in sim -->
    <nu>1</nu>                            <!-- include fraction nu of non-donor deaths in objective  -->
    <T0>3</T0>                            <!-- final days without vaccination. Must be >= 1 -->
    <dT>4</dT>                            <!-- days after t_n[0] in Lagr (t_LP). Set >= T to use all t -->
    <t_switch>20</t_switch>                <!-- day 2-area init policy switches 1->2. Set >= T for all 1 -->
    <p_k>1</p_k>                          <!-- max prop of avail vacc used in donor area -->
    <lambda_0>.1</lambda_0>              <!-- initial Lagrange multiplier for infection -->
    <phi>4</phi>                          <!-- exploration multiplier for lambda -->
    <epsilon_0>100</epsilon_0>            <!-- exploration tolerance for I -->
    <delta_I>0.001</delta_I>                  <!-- 0.1 termination tolerance for I -->
    <delta>0.1</delta>                    <!-- termination tolerance for lambda -->
    <beta>0.9</beta>                      <!-- convergence parameter -->
    <iter_lmt>20</iter_lmt>                <!-- iteration limit for I -->
    <iter_lmt_search>10</iter_lmt_search> <!-- iteration limit for lambda -->
  </params>
</data>